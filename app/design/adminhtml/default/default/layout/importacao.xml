        </reference>
    </importacao_adminhtml_importacao_index>

    <importacao_adminhtml_importacao_cadastrar translate="label">
        <label>Cadastrar Usuários na Árvore</label>
        <reference name="head">
            <action method="addCss"><stylesheet>jquery-ui.css</stylesheet></action>
            <action method="addJs"><script>jquery/jquery-1-12-4.js</script></action>
            <action method="addJs"><script>jquery/jquery-ui.js</script></action>
        </reference>
        <reference name="content">
            <block type="adminhtml/template" name="importacaocadastrar" template="importacao/cadastrar.phtml" />
        </reference>
    </importacao_adminhtml_importacao_cadastrar>


